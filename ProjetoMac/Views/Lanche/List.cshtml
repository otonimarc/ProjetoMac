@model IEnumerable<ProjetoMac.Models.Lanche>

<div>
    <h2>@ViewData["Titulo"]</h2>
</div>
    <hr/>
    <h3>@ViewBag.Total @ViewBag.TotalLanches</h3>
    <hr/>
    <h3>@TempData["Nome"]</h3>
<h2>@ViewData["Data"]</h2>

<div class="container">
    <div class="row">
    @foreach (var lanche in Model)
    {
        <div class="col-sm-4 col-md-4 col-lg-4">
            <p><img src="@lanche.ImagemUrl"></p>
            <h3> Preço: @lanche.Preco.ToString("c")</h3>
            <h4>@lanche.Nome</h4>
            <p>Descrição : @lanche.DescricaoCurta </p>
        </div>
    }
</div>
</div>